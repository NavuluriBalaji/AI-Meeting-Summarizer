{
  "version": 2,
  "builds": [
    {
      "src": "AI-Meeting-Summarizer-main/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "AI-Meeting-Summarizer-main/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/summarizer",
      "dest": "/AI-Meeting-Summarizer-main/index.html"
    },
    {
      "src": "/history",
      "dest": "/AI-Meeting-Summarizer-main/index.html"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
