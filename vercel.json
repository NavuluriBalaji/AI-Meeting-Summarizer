{
  "version": 2,
  "builds": [
    {
      "src": "AI-Meeting-Summarizer/src/**/*.{js,ts,jsx,tsx}",
      "use": "@vercel/node"
    },
    {
      "src": "AI-Meeting-Summarizer/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/AI-Meeting-Summarizer/index.html"
    }
  ]
}
